# Environment Variables for Production Deployment

# JWT Secret (CHANGE THIS!)
JWT_SECRET=mySecretKeyForJWTTokenGenerationAndValidation12345

# Google OAuth2 Credentials
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/agriproj

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# YOLOv3 Service URL
YOLO_SERVICE_URL=http://localhost:8000

# Frontend API URL (for production build)
VITE_API_URL=http://localhost:8080

# Grafana Admin Credentials
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin123
GRAFANA_URL=http://localhost:3000

# Application URLs
BACKEND_URL=http://localhost:8080
FRONTEND_URL=http://localhost:80

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:80,http://localhost:5173,http://localhost:5174
